{
  "version": 2,
  "builds": [
    { "src": "public/js/common.min.js", "use": "@now/static" },
    { "src": "public/js/main.min.js", "use": "@now/static" },
    { "src": "public/js/product.min.js", "use": "@now/static" },
    { "src": "app.js", "use": "@now/node-server" }
  ],
  "routes": [
    { "src": "/js/common.min.js", "dest": "public/js/common.min.js" },
    { "src": "/js/main.min.js", "dest": "public/js/main.min.js" },
    { "src": "/js/product.min.js", "dest": "public/js/product.min.js" },
    { "src": "/.*", "dest": "app.js" }
  ]
}
